
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div id="ghi">
    @Html.Action("GetAllProducts")
</div>

<div class="modal fade" id="moveto" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Move Product</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="mo">
                Sorry !!
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

@section ScriptRegion
    {
    <script>
        
        function MoveTo() {
            $("#moveto").modal("toggle");
        }
    </script>
}